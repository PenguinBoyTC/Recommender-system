import java.util.*;
public class test {
	public static void main(String[] args) {
		
	}
	public void per(String str) {
		if (str == null || str.length() == 0) {
			return;
		}
		StringBuilder sb = new StringBuilder();
		dfs(str, 0, sb);
	}
	private void dfs(String str, int level, StringBuilder sb) {
		if (level == str.length()) {
			System.out.println(sb.toString());
			return;
		}
		sb.append(str.charAt(level));
		dfs(str, level + 1, sb);
		sb.append(" ");
		dfs(str, level + 1, sb);
		sb.deleteCharAt(sb.length() - 1);
	}

}
